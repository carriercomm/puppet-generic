global
	log /dev/log daemon <%= haproxy_loglevel %>
	maxconn 32000
	ulimit-n 65536
	user haproxy
	group haproxy
	daemon
	stats socket /var/run/haproxy.sock uid haproxy

defaults
	log            global
	retries        5
	mode           http
	option         redispatch
	option         dontlognull
	option         http-server-close
	option         forwardfor
	balance        roundrobin
	maxconn        200000
	timeout client 5000
